{
    "name": "epk-technologies/symfony-bundle",
    "description": "EPK Symfony Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Jan Egert",
            "email": "jan.egert@epk-technologies.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-gmp": "*",
        "ext-iconv": "*",
        "ramsey/uuid": "^4.1",
        "symfony/console": "^5.3|6.*",
        "symfony/dotenv": "^5.3|6.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^5.3|6.*",
        "symfony/string": "^5.3|6.*",
        "symfony/translation-contracts": "^2.4",
        "symfony/yaml": "^5.3|6.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "8.0"
        }
    },
    "autoload": {
        "psr-4": {
            "EPKTechnologies\\EPKBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EPKTechnologies\\EPKBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.3"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^5.3",
        "symfony/css-selector": "^5.3",
        "symfony/phpunit-bridge": "^5.3"
    }
}
